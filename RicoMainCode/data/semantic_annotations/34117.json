        {
          "text": "Se verá en Puerto Rico pelea titular entre Acosta y Tanaka",
          "resource-id": "hr.apps.n207032405:id/item_title",
          "ancestors": [
            "android.widget.TextView",
            "android.view.View",
            "java.lang.Object"
          ],
          "clickable": false,
          "class": "android.support.v7.widget.AppCompatTextView",
